{% trans_default_domain "symbbfrontend" %}

{% extends getSymbbTemplateConfig('forum') ~ ':Forum:index.html.twig' %}



{% block symbb_forum_breadcrumb %}
    {{ getSymbbBreadcrumb(forum) }}
{% endblock %}

{% block symbb_forum_body %}

    <div class="symbb_forum_list">
        {{ symbBBForumMacro.showForumList(forum.getChildren(), 1) }}
        {% if forum.hasTopics() %}
            {{ symbBBForumMacro.showTopicList(forum) }}
        {% endif %}
    </div>
{% endblock %}

