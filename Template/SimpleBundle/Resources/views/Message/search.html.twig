{% extends 'FOSMessageBundle::layout.html.twig' %}

{% block fos_message_content %}

<div class="panel panel-primary">
  <div class="panel-heading">{% trans from 'symbb_frontend' %}search{% endtrans %}</div>
  <div class="panel-body">
    <p>{% transchoice threads|length with {'%num%': threads|length } from 'symbb_frontend' %}
        threads_found
    {% endtranschoice %}</p>

    {% include 'FOSMessageBundle:Message:threads_list.html.twig' with {'threads': threads} %}
  </div>
</div>

{% endblock %}