{% trans_default_domain "symbbfrontend" %}
<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
  <head>
    <title>{% block title %}{% endblock %}</title>
    <meta http-equiv="Content-Language" content="{{ app.request.locale }}" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    
    {% block stylesheets %} 
    {% endblock %}
        
    
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://code.jquery.com/jquery.js"></script>
    <!-- Latest compiled and minified JavaScript -->
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.2/js/bootstrap.min.js"></script>
    </head>

    <body>
        {% block symbb_forum_navi %} 
        
        {% endblock %}
            
        <div class="symbb_forum_breadcrumb">
        {% block symbb_forum_breadcrumb %}

        {% endblock %}
        </div>
        
        <div class="symbb_forum_body">
        {% block symbb_forum_body %}

        {% endblock %}
        </div>
    </body>
</html>