{% trans_default_domain "symbb_frontend" %}

{% extends getSymbbTemplate('forum') ~ '::layout.html.twig' %}

{% block stylesheets %} 
    {{ parent() }}
    {% stylesheets 
        '@SymBBTemplateDefaultBundle/Resources/public/css/forum.css' filter='?uglifycss'
        '@SymBBTemplateDefaultBundle/Resources/public/css/topic.css' filter='?uglifycss'
     %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    {{ executeSymbbEvent('forum.template.stylesheets') }}
{% endblock %}
    
{% block javascripts %} 
    {{ parent() }}
    {% javascripts 
        '@SymBBTemplateDefaultBundle/Resources/public/js/datepicker/jquery.datetimepicker.js' filter='?uglifyjs2'
        '@SymBBTemplateDefaultBundle/Resources/public/js/form/collection.js' filter='?uglifyjs2'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {{ executeSymbbEvent('forum.template.javascripts') }}
{% endblock %}
    
{% block symbb_breadcrumb %}
    <symbb-breadcrumb></symbb-breadcrumb>
{% endblock %}
    
    
   
