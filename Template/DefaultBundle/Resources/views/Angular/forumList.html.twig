{% trans_default_domain "symbb_frontend" %}
<div ng-repeat="category in categoryList">

    <div class="anker-group">
        <div class="anker" id="category_[[category.id]]"></div>
    </div>
    <div class="symbb_action_row">
        <div class="title" symbb-link="forum_list" param-id="[[category.id]]">[[category.name]]</div>
        <div class="arrow"></div>
        <div class="clear"></div>
    </div>

    <div class="symbb_category_block_container col-lg-4 col-md-6 col-sm-12" ng-repeat="object in category.children" symbb-link="forum_list" param-id="[[object.id]]">
        {{ include('SymBBTemplateDefaultBundle:Angular:forumListCard.html.twig') }}
    </div>

    <div class="clear"></div>
</div>

<div ng-show="forumListCheck">
    <div class="anker-group">
        <div class="anker" id="forums"></div>
    </div>
    <div class="symbb_action_row">
        <div class="title"><a href="#forums">{{ 'Forums'|trans() }}</a></div>
        <div class="arrow"></div>
        <div class="clear"></div>
    </div>
</div>

<div ng-if="">
    
</div>
<div class="symbb_category_block_container col-lg-4 col-md-6 col-sm-12" ng-repeat="object in forumList" symbb-link="forum_list" param-id="[[object.id]]">
    {{ include('SymBBTemplateDefaultBundle:Angular:forumListCard.html.twig') }}
</div>