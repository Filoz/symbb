<div symbb-bbcode-editor class="symbb_bbcodes symbb_header_bg">
    <div class="symbb_bbcodes_group">
        {% for key, bbcode in getSymBBBBCodes(bbcodeset) %}
            <div class="symbb_bbcode_btn" data-tag-code="{{ bbcode.getButtonRegex() }}" class="btn btn-default btn-sm symbb_bbcode_code" title="{{ bbcode.getName() }}"><img alt="{{ bbcode.getName() }}" title="{{ bbcode.getName() }}" src="{{ bbcode.getImage() }}" /></div>
        {% endfor %} 
    </div>
    <div class="clear"></div>
</div>
<div class="symbb_editor" ng-transclude>
    
</div>